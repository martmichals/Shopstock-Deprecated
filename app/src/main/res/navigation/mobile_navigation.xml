<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_mapexplore">

    <fragment
        android:id="@+id/navigation_mapexplore"
        android:name="com.example.shopstock.ui.mapexplore.MapExploreFragment"
        android:label="@string/title_mapexplore"
        tools:layout="@layout/fragment_mapexplore" />

    <fragment
        android:id="@+id/navigation_report"
        android:name="com.example.shopstock.ui.report.ReportFragment"
        android:label="@string/title_report"
        tools:layout="@layout/fragment_report" />

    <fragment
        android:id="@+id/navigation_search"
        android:name="com.example.shopstock.ui.search.SearchFragment"
        android:label="@string/title_search"
        tools:layout="@layout/fragment_search" />
</navigation>
